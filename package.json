{
  "name": "rpgym",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --go",
    "android": "expo start --go --android",
    "ios": "expo start --go --ios",
    "web": "expo start --go --web",
    "prettier-format": "prettier --config .prettierrc '**/*.{json,js,jsx,ts,tsx,css,scss,md}' --write",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
    "lint-ci": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix --config ./ci-eslint.json",
    "lint-and-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
    "test": "jest --watchAll",
    "test-ci": "jest --passWithNoTests",
    "build": "npx expo export",
    "deploy-eas": "eas update --non-interactive --auto --channel preview",
    "deploy-web": "npx expo export && firebase deploy",
    "postinstall": "husky install"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json }": "eslint --config ./ci-eslint.json --fix",
    "*": "prettier --ignore-unknown --config .prettierrc"
  },
  "dependencies": {
    "@expo-google-fonts/noto-sans": "^0.2.3",
    "@expo-google-fonts/work-sans": "^0.2.3",
    "@expo/vector-icons": "^13.0.0",
    "@miblanchard/react-native-slider": "^2.3.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/native": "^6.0.2",
    "date-fns": "^2.30.0",
    "expo": "^49.0.0",
    "expo-constants": "^14.4.2",
    "expo-dev-client": "~2.4.6",
    "expo-font": "~11.4.0",
    "expo-image": "~1.3.2",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-router": "2.0.0",
    "expo-splash-screen": "~0.20.4",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.10",
    "expo-web-browser": "~12.3.2",
    "firebase": "^9.22.1",
    "metro": "~0.76.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-calendars": "^1.1299.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.1.6",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "jest-expo": "^49.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
